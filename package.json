    "@types/nodemon": "^1.19.0",
    "@types/tmi.js": "^1.4.0",
    "jest": "^25.1.0",
    "nodemon": "^2.0.2",
    "rimraf": "^3.0.2",
    "ts-loader": "^6.2.1",
